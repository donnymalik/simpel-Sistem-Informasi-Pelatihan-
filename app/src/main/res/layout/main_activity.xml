            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:id="@+id/epublik"
                android:src="@drawable/speech_bubble"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_marginTop="10dp"
                android:textSize="12sp"
                android:layout_height="wrap_content"
                android:layout_below="@+id/epublik"
                android:layout_centerHorizontal="true"
                android:textColor="#292929"
                android:text="e-Publik"/>
        </RelativeLayout>
    </FrameLayout>
    <FrameLayout
        app:layout_columnWeight="1"
        app:layout_rowWeight="1"
        android:background="#d6acac"
        android:onClick="webviewinfosewa">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:id="@+id/infosewa"
                android:src="@drawable/money"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_marginTop="10dp"
                android:textSize="12sp"
                android:layout_height="wrap_content"
                android:layout_below="@+id/infosewa"
                android:layout_centerHorizontal="true"
                android:textColor="#292929"
                android:text="Infosewa"/>
        </RelativeLayout>
    </FrameLayout>

    <FrameLayout
        app:layout_columnWeight="1"
        app:layout_rowWeight="1"
        android:background="#f87f2a"
        android:onClick="webviewjadwallab">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
